    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "lib": ["es2015", "dom"],
    "downlevelIteration": true
  },
  "exclude": ["./node_modules"]
}
